<p id="notice"><%= notice %></p>

<h1>Listing Wish List Items</h1>

<table>
  <thead>
    <tr>
      <th>Wishlist</th>
      <th>Product</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wish_list_items.each do |wish_list_item| %>
      <tr>
        <td><%= wish_list_item.wishlist_id %></td>
        <td><%= wish_list_item.product_id %></td>
        <td><%= link_to 'Show', wish_list_item %></td>
        <td><%= link_to 'Edit', edit_wish_list_item_path(wish_list_item) %></td>
        <td><%= link_to 'Destroy', wish_list_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Wish list item', new_wish_list_item_path %>
